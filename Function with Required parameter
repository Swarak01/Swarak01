//Function and Method
 //With Required parameter
 
  void main(){
  
    rectArea(2,6);
    int p=perimeter(4,7);
    print("Perimeter of rectangle $p");
  }

void rectArea(int l, int b)
{
  int a =l*b;
  
  print("Area of rectangle $a");
}

int perimeter(int l, int b)
{
  int p= 2*(l+b);
  return p;
 
} 

Output:
Area of rectangle 12
Perimeter of rectangle 22
